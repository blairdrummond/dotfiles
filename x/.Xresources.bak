cursor: fg
border: bg
mouse_background: blu
mouse_foreground: fg
highlight: grn

st.cursorColor:  blu

st.font: terminus-12
! st.termname:			xterm-256color
! st.shell:			/bin/zsh
! st.bellvolume:			100
st.borderpx:			5
! st.tabspaces:			8
st.cwscale:			1.1
st.chscale:			1.0



rofi.color-window:    bg,   fg
rofi.color-normal:    bg,   fg,   bg,   fg,   bg
rofi.color-urgent:    bg,   fg,   bg,   fg,   bg
rofi.color-active:    bg,   fg,   bg,   fg,   bg



rofi.modi: run,ssh,window,drun
! rofi.fullscreen: true
! rofi.eh: 2
! rofi.opacity: 85
! rofi.prompt: ""
rofi.width: 30
! rofi.lines: 5
rofi.font: Terminus 9
! rofi.bw: 0
! rofi.fake-transparency: true
! rofi.location: 0
rofi.padding: 10
rofi.terminal: urxvtc
rofi.ssh-set-title: true
rofi.disable-history: false
rofi.sidebar-mode: true
rofi.separator-style: none
rofi.fuzzy: false
rofi.hide-scrollbar: true
rofi.line-margin: 0
rofi.mesg: ""
rofi.color-enabled: true
rofi:fixed-num-lines: false





! clean up urxvt, add keybinds
URxvt*intensityStyles: false
URxvt*scrollBar: false
! URxvt*keysym.Shift-Up: command:\033]720;1\007
! URxvt*keysym.Shift-Down: command:\033]721;1\007
! URxvt*resize-font.smaller: C-Down
! URxvt*resize-font.bigger: C-Up
! URxvt*iso14755: false
! URxvt*iso14755_52: false
! URxvt*geometry:  100x30

URxvt*urgentOnBell: true

! URxvt*dynamicColors: on

! URxvt*meta8: ignore

URxvt*internalBorder: 10
URxvt*lineSpace: 3
URxvt*letterSpace: 1

! make otf / ttf fonts look pretty.  does not matter if you are using a bitmap font.
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! do not scroll with output

URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true


URxvt*perl-lib:  /home/blair/scripts/xbin/
URxvt.perl-ext-common: clipboard

URxvt*keysym.C-C: perl:clipboard:copy
URxvt*keysym.C-V: perl:clipboard:paste
URxvt*keysym.C-A-V: perl:clipboard:paste_escaped

URxvt.cursorUnderline: true





